Phone_WriteMessage(playerid, number)
{
	CellphoneData[playerid][cScreen] = PHONE_PLAYER_MESSAGE_WRITTING;

	Phone_UpdateScreen(playerid, "white");

	for(new i = PHONE_MESSAGER_WRITE_TOP_BOX; i < PHONE_MESSAGER_WRITE_SEND; i++)
		TextDrawShowForPlayer(playerid, Global_PhoneSystem[i]);

	for(new i = PHONE_MESSAGER_WRITE_NUMBER; i <= PHONE_MESSAGER_WRITE_MESSAGE; i++)
		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][i]);

	CellphoneData[playerid][cWriteMessage] = EOS;

