//Atalho para a callback principal
Phone_PlayerClick(playerid, PlayerText:textid)
{	
	for(new i = 0; i < 5; i++)
	{
		if(textid == Player_PhoneSystem[playerid][PHONE_MESSAGER_NAME_1 + i])
		{
			if(!CellphoneData[playerid][cSignal])
				return 0;

			Hide_MessagerApp(playerid);

			Show_MessagerApp_Info(playerid);
			SetPVarInt(playerid, "VAR_Message_Number", CellphoneData[playerid][cList][i]);

			MessagerApp_Info_Page(playerid, 1);

			break;
		}
	}

	if(textid == Player_PhoneSystem[playerid][PHONE_CALLER_CONTACTS])
	{
		if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLER_CONTACTS)
			return 0;

		Hide_CallerApp_Recents(playerid);
		Hide_CallerApp_RecentInfo(playerid);
		Hide_CallerApp_Keyboard(playerid);
		Hide_CallerApp_ContactInfo(playerid);

		Show_CallerApp_Contacts(playerid);
	}
	else if(textid == Player_PhoneSystem[playerid][PHONE_CALLER_RECENTS])
	{
		if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLER_RECENTS)
			return 0;

		Hide_CallerApp_Keyboard(playerid);
		Hide_CallerApp_Contacts(playerid);
		Hide_CallerApp_ContactInfo(playerid);
		Hide_CallerApp_RecentInfo(playerid);

		Show_CallerApp_Recents(playerid);
	}
	else if(textid == Player_PhoneSystem[playerid][PHONE_CALLER_KEYBOARD])
	{
		if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLER_KEYBOARD)
			return 0;

		Hide_CallerApp_Recents(playerid);
		Hide_CallerApp_RecentInfo(playerid);
		Hide_CallerApp_Contacts(playerid);
		Hide_CallerApp_ContactInfo(playerid);

		Show_CallerApp_Keyboard(playerid);
	}	
	else if(textid == Player_PhoneSystem[playerid][PHONE_CALLER_RECENT_LEFT_BOX] || textid == Player_PhoneSystem[playerid][PHONE_CALLER_RECENT_LEFT_TEXT])
	{
		if(CellphoneData[playerid][cScreen] != PHONE_PLAYER_CALLER_RECENTS)
			return 0;

		SetPVarInt(playerid, "VAR_Recents_Status", 0);

		CallerApp_Recent_Page(playerid, 1);
	}
	else if(textid == Player_PhoneSystem[playerid][PHONE_CALLER_RECENT_RIGHT_BOX] || textid == Player_PhoneSystem[playerid][PHONE_CALLER_RECENT_RIGHT_TEXT])
	{
		if(CellphoneData[playerid][cScreen] != PHONE_PLAYER_CALLER_RECENTS)
			return 0;

		SetPVarInt(playerid, "VAR_Recents_Status", 1);

		CallerApp_Recent_Page(playerid, 1);
	}
	else if(textid == Player_PhoneSystem[playerid][PHONE_MESSAGER_WRITE_BOX])
	{
		if(CellphoneData[playerid][cScreen] != PHONE_PLAYER_MESSAGE_WRITTING)
			return 0;

		Dialog_Show(playerid, dMessagerApp_WriteMessage, DIALOG_STYLE_INPUT, "Escrever mensagem", "Digite a mensagem que você deseja enviar para o número escolhido.\nLimites: 75 caracteres.", ">>", "<<");
	}
	else if(textid == Player_PhoneSystem[playerid][PHONE_SETTING_MAIN_BUTTON_AIRPLANE])
	{
		if(CellphoneData[playerid][cScreen] != PHONE_PLAYER_SETTING)
			return 0;

		CellphoneData[playerid][cAirPlaneMode] = CellphoneData[playerid][cAirPlaneMode] == 1 ? 0 : 1;

		if(CellphoneData[playerid][cAirPlaneMode])
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_SETTING_MAIN_BUTTON_AIRPLANE], "mdl-2000:button-slideon");
		else
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_SETTING_MAIN_BUTTON_AIRPLANE], "mdl-2000:button-slideoff");
	}

	return 1;
}

//Atualizar a tela do Caller App
