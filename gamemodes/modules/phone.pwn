		if(page + 1 != lastpage)
			TextDrawShowForPlayer(playerid, Global_PhoneSystem[PHONE_MESSAGER_NEXT_PAGE]);

		if(page + 1 >= 2)
			TextDrawShowForPlayer(playerid, Global_PhoneSystem[PHONE_MESSAGER_BACK_PAGE]);
	}

	return 1;
}

this::MessagerApp_Info_List(playerid, page)
{
	new rows;
	cache_get_row_count(rows);

	new message[76], sender_number, receiver_number, date, ano[2], mes[2], dia[2], hora, minuto, segundo, string[16];
	getdate(ano[1], mes[1], dia[1]);

	for(new i = 0; i < rows && i < 3; i++)
	{
		cache_get_value_name(i, "message", message, 76);
		cache_get_value_name_int(i, "sender_number", sender_number);
		cache_get_value_name_int(i, "receiver_number", receiver_number);
		cache_get_value_name_int(i, "date", date);

		TimestampToDate(date, ano[0], mes[0], dia[0], hora, minuto, segundo);

		if(sender_number == PlayerData[playerid][pCellphone])
			MessagerApp_Create_Box(playerid, i, 1, message);
		
		else
			MessagerApp_Create_Box(playerid, i, 0, message);

		if(dia[0] == dia[1] && mes[0] == mes[1])
			format(string, sizeof(string), "%02d:%02d", hora, minuto);
		else
			format(string, sizeof(string), "%d/%d", dia[0], mes[0]);

		for(new j = 0; j < strlen(message); j++)
		{
			if(message[j] == ' ')
				message[j] = '_';
		}

		PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_INFO_DATE_1 + i], string);
		PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_INFO_MSG_1 + i], FixASCII(MessagerApp_FormatMsg(message)));

		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_INFO_BOX_1 + i]);
		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_INFO_DATE_1 + i]);
		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_INFO_MSG_1 + i]);
	}

	PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_INFO_NAME], FixASCII(Phone_GetContact(playerid, GetPVarInt(playerid, "VAR_Message_Number"))));
	PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_INFO_NAME]);

	//pagination system
	new query[210], total_rows;
	mysql_format(this, query, sizeof(query), "SELECT * FROM `cellphone_messages` WHERE (sender_number = '%d' AND receiver_number = '%d') OR (sender_number = '%d' and receiver_number = '%d')", 
	PlayerData[playerid][pCellphone], GetPVarInt(playerid, "VAR_Message_Number"), GetPVarInt(playerid, "VAR_Message_Number"), PlayerData[playerid][pCellphone]);
	new Cache:result = mysql_query(this, query);
	total_rows = cache_num_rows() - 1;
	cache_delete(result);

	if(total_rows >= 3)
	{
		new lastpage;

		lastpage = (total_rows / 3) + 1;

		//limitador
		if(lastpage > 20)
			lastpage = 20;

		if(page + 1 != lastpage)
			TextDrawShowForPlayer(playerid, Global_PhoneSystem[PHONE_MESSAGER_INFO_NEXT_PAGE]);

		if(page + 1 >= 2)
			TextDrawShowForPlayer(playerid, Global_PhoneSystem[PHONE_MESSAGER_INFO_BACK_PAGE]);
	}

	return 1;
}

