//credo, que negocio mal feito.. faz um melhor pra essa bosta ae aiueauieaeu
MessagerApp_FormatMsg(msg[])
{
	new value[100], length = strlen(msg);

	format(value, sizeof(value), msg);

	if(length > 25)
	{
		for(new i = length; i >= 0; i--) if(i >= 25)
		{
			if(i % 25 == 0 && msg[i] != EOS)
			{
				if(msg[i] == '_')
					strins(value, "~n~", i + 1);
				else if(msg[i - 1] == '_')
					strins(value, "~n~", i);
				else
					strins(value, "-~n~", i);
			}
		}
	}

	return value;
}

//Retornar se o texto digitado é válido para ser inserido no textdraw
IsValidTextDrawName(string[])
{
	static const BlockedLetters[] = {34, 37, 39, 92, 94, 96, 123, 124, 125, 126};

	new len = strlen(string);

	for(new i = 0; i < len; i++)
	{
		for(new j = 0; j < sizeof(BlockedLetters); j++)
		{
			if(string[i] == BlockedLetters[j])
				return 0;
		}
	}

	return 1;
}

