				}
				else
				{
					PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL]);

					PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL_TEXT], -1);
					PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL_TEXT]);
				}				
			}
			case 1:
			{
				PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL_TEXT]);

				PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL], "mdl-2000:white-signal-1");
				PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL]);
			}
			case 2:
			{
				PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL_TEXT]);

				PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL], "mdl-2000:white-signal-2");
				PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL]);
			}
