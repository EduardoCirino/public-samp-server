this::MessagerApp_List(playerid, page)
{
	if(!CellphoneData[playerid][cSignal])
		return 1;

	new rows;
	cache_get_row_count(rows);

	new sender_number, receiver_number, message[76], count;

	for(new i = 0; i < rows && i < 5; i++)
	{
		count++;

		cache_get_value_name(i, "message", message, 76);
		cache_get_value_name_int(i, "sender_number", sender_number);
		cache_get_value_name_int(i, "receiver_number", receiver_number);

		if(sender_number == PlayerData[playerid][pCellphone])
			CellphoneData[playerid][cList][i] = receiver_number;

		else
			CellphoneData[playerid][cList][i] = sender_number;

		for(new j = 0; j < strlen(message); j++)
		{
			if(message[j] == ' ')
				message[j] = '_';
		}

		if(strlen(message) > 25)
		{
			format(message, sizeof(message), "%.25s...", message);
		}

		PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_PREVIEW_1 + i], FixASCII(message));
	}

	for(new i = 0; i < count; i++)
	{
		PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_NAME_1 + i], FixASCII(Phone_GetContact(playerid, CellphoneData[playerid][cList][i])));
		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_NAME_1 + i]);
		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_PREVIEW_1 + i]);
		TextDrawShowForPlayer(playerid, Global_PhoneSystem[PHONE_MESSAGER_TRACE_1 + i]);
	}

	//pagination system
	new query[550], total_rows;

	mysql_format(this, query, sizeof(query), "select cellphone_messages.* from cellphone_messages join (select user, max(date) m from ((select uniqueid, receiver_number user, date from cellphone_messages where sender_number=%d) union (select uniqueid, sender_number user, date from cellphone_messages where receiver_number=%d))",
	PlayerData[playerid][pCellphone], PlayerData[playerid][pCellphone]);
	mysql_format(this, query, sizeof(query), "%s t1 group by user) t2 on ((sender_number=%d and receiver_number=user) or (sender_number=user and receiver_number=%d)) and (date = m) order by date desc", query, PlayerData[playerid][pCellphone], PlayerData[playerid][pCellphone]);
	new Cache:result = mysql_query(this, query);
	total_rows = cache_num_rows() - 1;
	cache_delete(result);

	if(total_rows >= 5)
	{
		new pageinfo[10], lastpage;

		lastpage = (total_rows / 5) + 1;

		//limitador
		if(lastpage > 10)
			lastpage = 10;

		format(pageinfo, sizeof(pageinfo), "%d_/_%d", page + 1, lastpage);

