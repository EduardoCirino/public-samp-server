//Função do botão home.
Phone_Home(playerid, force = 0)
{
	if(!CellphoneData[playerid][cOpened])
		return 0;

	if(!force)
	{
		if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLING || CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALL)
			return 0;
	}

	CellphoneData[playerid][cScreen] = PHONE_PLAYER_MAIN;

	for(new i = PHONE_BLOCKED_ICON; i < MAX_GLOBAL_PHONE_TEXTDRAW; i++)
		TextDrawHideForPlayer(playerid, Global_PhoneSystem[i]);

	for(new i = PHONE_CALLER_RECENTS; i < MAX_PLAYER_PHONE_TEXTDRAW; i++)
		PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][i]);

	Phone_UpdateScreen(playerid, "default");
	
	Show_MainScreen(playerid);
	Phone_Clear(playerid);

	return 1;
}

//Abre o aplicativo selecionado
Phone_OpenApp(playerid, appid)
{
	if(CellphoneData[playerid][cScreen] != PHONE_PLAYER_MAIN)
		return 0;

	Hide_MainScreen(playerid);

	switch(appid)
	{
		case APP_CALLER:
		{
			Show_CallerApp(playerid);
		}
		case APP_MESSAGER:
		{
			Show_MessagerApp(playerid);
		}
		case APP_CALCULATOR:
		{
			//xD
		}
		case APP_SETTINGS:
		{
			Show_SettingApp(playerid);
		}
	}

	return 1;
}

//Limpar viariaveis
Phone_Clear(playerid, all = 0)
{
	if(all)
	{
		new callerid;
		if((callerid = CellphoneData[playerid][cCaller]) != INVALID_PLAYER_ID && PlayerData[callerid][pLogged])
		{
			Phone_EndCall(playerid, "chamada_desconectada", 7);
			Phone_EndCall(callerid, "chamada_desconectada", 7);
		}

		CellphoneData[playerid][cOpened] = false;
		CellphoneData[playerid][cScreen] = PHONE_PLAYER_LOCKED;
	}

	PlayerData[playerid][pEmergency] = 0;
	PlayerData[playerid][pBlackMarket] = 0;
	PlayerData[playerid][pInsurance] = 0;
	CellphoneData[playerid][cCaller] = INVALID_PLAYER_ID;
	CellphoneData[playerid][cNumber] = -1;
	CellphoneData[playerid][cDuration] = 0;
	KillTimer(CellphoneData[playerid][cTimerCalling]);
	KillTimer(CellphoneData[playerid][cTimerCallFinished]);

	for(new i = 0; i < 10; i++) CellphoneData[playerid][cList][i] = -1;

	return 1;
}

