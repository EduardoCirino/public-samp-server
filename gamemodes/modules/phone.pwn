			//text
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CONTACTS_TEXT], -1364283649);
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_KEYBOARD_TEXT], 562560767);
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_RECENTS_TEXT], -1364283649);
		}
		case PHONE_PLAYER_CALLER_CONTACTS:
		{
			//icons
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CONTACTS], "mdl-2000:contacts-selected");
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_KEYBOARD], "mdl-2000:keyboard");
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_RECENTS], "mdl-2000:recents");

			//text
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CONTACTS_TEXT], 562560767);
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_KEYBOARD_TEXT], -1364283649);
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_RECENTS_TEXT], -1364283649);
		}
		case PHONE_PLAYER_CALLER_RECENTS:
		{
			//icons
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CONTACTS], "mdl-2000:contacts");
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_KEYBOARD], "mdl-2000:keyboard");
			PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_RECENTS], "mdl-2000:recents-selected");

			//text
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CONTACTS_TEXT], -1364283649);
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_KEYBOARD_TEXT], -1364283649);
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_RECENTS_TEXT], 562560767);
		}
	}

	PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CONTACTS_TEXT]);
	PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_KEYBOARD_TEXT]);
	PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_RECENTS_TEXT]);

	return 1;
}

