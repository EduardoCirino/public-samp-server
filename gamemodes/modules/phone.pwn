//Fechar celular
Phone_Close(playerid)
{
	if(!CellphoneData[playerid][cOpened])
		return 0;

	for(new i = 0; i < MAX_PLAYER_PHONE_TEXTDRAW; i++)
		PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][i]);

	for(new i = 0; i < MAX_GLOBAL_PHONE_TEXTDRAW; i++)
		TextDrawHideForPlayer(playerid, Global_PhoneSystem[i]);

	Phone_Clear(playerid, 1);

	return 1;
}

//Atualizar celular
Phone_Update(playerid)
{
	if(!CellphoneData[playerid][cOpened])
		return 0;

	//Check sinal
	Phone_UpdateSignal(playerid);

	new string[40], dia, mes, ano, hora, minuto, segundo;
	getdate(ano, mes, dia);
	gettime(hora, minuto, segundo);

	if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_LOCKED)
	{
		format(string, sizeof(string), "%02d:%02d", hora, minuto);
		TextDrawSetString(Global_PhoneSystem[PHONE_BLOCKED_HOUR], string);

		new month[12];
		switch(mes)
		{
			case 1: month = "Janeiro";
			case 2: month = "Fevereiro";
			case 3: month = "Março";
			case 4: month = "Abril";
			case 5: month = "Maio";
			case 6: month = "Junho";
			case 7: month = "Julho";
			case 8: month = "Agosto";
			case 9: month = "Setembro";
			case 10: month = "Outubro";
			case 11: month = "Novembro";
			case 12: month = "Dezembro";
		}

		new weekday[20];
		switch(((gettime() + 334800) / 86400 - (gettime() + 334800) / 86400 / 7 * 7))
		{
			case 0: weekday = "Domingo";
			case 1: weekday = "Segunda-feira";
			case 2: weekday = "Terça-feira";
			case 3: weekday = "Quarta-feira";
			case 4: weekday = "Quinta-feira";
			case 5: weekday = "Sexta-feira";
			case 6: weekday = "Sábado";
		}

		format(string, sizeof(string), "%d_de_%s~n~%s", dia, FixASCII(month), FixASCII(weekday));
		TextDrawSetString(Global_PhoneSystem[PHONE_BLOCKED_DATE], string);
	}
	else
	{
		if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_MAIN || CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLING || CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALL)
		{
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_HOUR], -1);
		}
		else
		{
			PlayerTextDrawColor(playerid, Player_PhoneSystem[playerid][PHONE_HOUR], 255);
		}

		format(string, sizeof(string), "%02d:%02d", hora, minuto);
		PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_HOUR], string);
		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_HOUR]);

		if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALL && !GetPVarInt(playerid, "VAR_Call_Finished"))
		{
			if(CellphoneData[playerid][cCaller] != INVALID_PLAYER_ID || CellphoneData[playerid][cNumber] != -1)
			{
				CellphoneData[playerid][cDuration]++;
				GetElapsedTime(CellphoneData[playerid][cDuration], hora, minuto, segundo);
				format(string, sizeof(string), "%02d:%02d", minuto, segundo);
				PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CALL_STATUS], string);
			}

			if(!CellphoneData[playerid][cSignal])
			{
				Phone_EndCall(playerid, "chamada_finalizada");
				
				new targetid;

				if((targetid = CellphoneData[playerid][cCaller]) != INVALID_PLAYER_ID)
					Phone_EndCall(targetid, "chamada_finalizada");
			}
		}
	}

	return 1;
}

//Atualizar sinal do celular
Phone_UpdateSignal(playerid)
{
	new Float:pX, Float:pY, Float:pZ, Float:final_signal = 0.0, towerid;
	GetPlayerLocationEx(playerid, pX, pY, pZ);

	foreach(new i : Towers) if(TowerData[i][towerExists])
	{
		if(IsPointInRangeOfPoint(pX, pY, pZ, TowerData[i][towerX], TowerData[i][towerY], TowerData[i][towerZ], TowerData[i][towerSignal]))
		{
			new Float:signal = GetDistanceBetweenTwoPoints(pX, pY, pZ, TowerData[i][towerX], TowerData[i][towerY], TowerData[i][towerZ]);
			signal = TowerData[i][towerSignal] - signal;

			if(signal > final_signal)
			{
				final_signal = signal;
				towerid = i;
			}
		}
	}

	if(final_signal >= (TowerData[towerid][towerSignal] * 0.7))
	{
		CellphoneData[playerid][cSignal] = 4;
	}
	else if(final_signal >= (TowerData[towerid][towerSignal] * 0.45))
	{
		CellphoneData[playerid][cSignal] = 3;
	}
	else if(final_signal >= (TowerData[towerid][towerSignal] * 0.20))
	{
		CellphoneData[playerid][cSignal] = 2;
	}
	else if(final_signal >= (TowerData[towerid][towerSignal] * 0.025))
	{
		CellphoneData[playerid][cSignal] = 1;
	}
	else
	{
		CellphoneData[playerid][cSignal] = 0;
	}

	if(PlayerData[playerid][pJailed] || CellphoneData[playerid][cAirPlaneMode])
		CellphoneData[playerid][cSignal] = 0;

	if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_LOCKED || CellphoneData[playerid][cScreen] == PHONE_PLAYER_MAIN || CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLING || CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALL)
	{
		switch(CellphoneData[playerid][cSignal])
		{			
			case 0:
			{
				if(CellphoneData[playerid][cAirPlaneMode])
				{
					PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL_TEXT]);

					PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL], "mdl-2000:white-airplane");
