MessagerApp_Adjust_WriteBox(playerid, msg[])
{
	switch(strlen(msg))
	{
		case 0..25:
		{
			PlayerTextDrawTextSize(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_WRITE_BOX], 58.000000, 9.879997);
		}
		case 26..50:
		{
			PlayerTextDrawTextSize(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_WRITE_BOX], 58.000000, 15.939997);
		}
		case 51..75:
		{
			PlayerTextDrawTextSize(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_WRITE_BOX], 58.000000, 22.000000);
		}
	}

	for(new i = 0; i < strlen(msg); i++)
	{
		if(msg[i] == ' ')
			msg[i] = '_';
	}

	PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_WRITE_MESSAGE], FixASCII(MessagerApp_FormatMsg(msg)));
	PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_MESSAGER_WRITE_BOX]);

	return 1;
}