//Sistema de Ligação
Phone_Call(playerid, number)
{
	if(PlayerData[playerid][pCellphone] == number)
		return SendWarningMessage(playerid, "Erro de discagem. Não é possível ligar para o seu próprio número telefônico.");

	SetPVarInt(playerid, "VAR_Call_Finished", 0);

	new targetid, delay_to_call = CellphoneData[playerid][cSignal] > 0 ? 10000 / CellphoneData[playerid][cSignal] : 2000;

	CellphoneData[playerid][cCaller] = INVALID_PLAYER_ID;
	CellphoneData[playerid][cDuration] = 0;

	if((targetid = GetNumberOwner(number)) != INVALID_PLAYER_ID)
		CellphoneData[playerid][cCaller] = targetid;

	CellphoneData[playerid][cNumber] = number;

	SetPlayerCellphoneAction(playerid, true);

	for(new i = PHONE_BLOCKED_ICON; i < MAX_GLOBAL_PHONE_TEXTDRAW; i++)
		TextDrawHideForPlayer(playerid, Global_PhoneSystem[i]);

	for(new i = PHONE_CALLER_RECENTS; i < MAX_PLAYER_PHONE_TEXTDRAW; i++)
		PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][i]);

	CellphoneData[playerid][cScreen] = PHONE_PLAYER_CALLING;
	CellphoneData[playerid][cTimerCalling] = SetTimerEx("CallerApp_Call_Number", delay_to_call, false, "dd", playerid, number);

	Phone_UpdateScreen(playerid, "call");

	PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CALL_NAME], FixASCII(Phone_GetContact(playerid, number)));
	PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CALL_STATUS], "ligando");

	PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CALL_NAME]);
	PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CALL_STATUS]);
	TextDrawShowForPlayer(playerid, Global_PhoneSystem[PHONE_CALLER_CALL_END]);

	return 1;
}

Phone_EndCall(playerid, reason[], stats = 1)
{
	if(CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLING || CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALL)
	{
		PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CALL_STATUS], FixASCII(reason));
		PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_CALLER_CALL_STATUS]);

		TextDrawHideForPlayer(playerid, Global_PhoneSystem[PHONE_CALLER_CALL_ACCEPT]);
		TextDrawHideForPlayer(playerid, Global_PhoneSystem[PHONE_CALLER_CALL_DECLINE]);
		TextDrawHideForPlayer(playerid, Global_PhoneSystem[PHONE_CALLER_CALL_END]);

		SetPVarInt(playerid, "VAR_Call_Finished", 1);
		Phone_AddRecent(playerid, CellphoneData[playerid][cNumber], gettime(), CellphoneData[playerid][cDuration], stats);

		SetPlayerCellphoneAction(playerid, false);
		CellphoneData[playerid][cTimerCallFinished] = SetTimerEx("CallerApp_Call_Finished", 2000, false, "d", playerid);

		PlayerData[playerid][pEmergency] = 0;
		PlayerData[playerid][pBlackMarket] = 0;
		PlayerData[playerid][pInsurance] = 0;
		CellphoneData[playerid][cCaller] = INVALID_PLAYER_ID;
		CellphoneData[playerid][cNumber] = -1;
	}

	return 1;
}

Phone_AcceptCall(playerid, callerid)
{
	CellphoneData[playerid][cScreen] = PHONE_PLAYER_CALL;
	CellphoneData[callerid][cScreen] = PHONE_PLAYER_CALL;

	SetPlayerCellphoneAction(playerid, true);

	KillTimer(CellphoneData[playerid][cTimerCalling]);

	TextDrawShowForPlayer(playerid, Global_PhoneSystem[PHONE_CALLER_CALL_END]);

	TextDrawHideForPlayer(playerid, Global_PhoneSystem[PHONE_CALLER_CALL_ACCEPT]);
	TextDrawHideForPlayer(playerid, Global_PhoneSystem[PHONE_CALLER_CALL_DECLINE]);

	return 1;
}

Phone_DeclineCall(playerid, callerid)
{
	Phone_EndCall(playerid, "chamada_rejeitada", 2);
	Phone_EndCall(callerid, "chamada_rejeitada", 2);

	return 1;
}

Phone_GetCall(playerid, &targetid, &targetnumber)
{
