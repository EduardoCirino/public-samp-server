this::CallerApp_Call_Number(playerid, number)
{
	if(!CellphoneData[playerid][cSignal] || number == PlayerData[playerid][pCellphone])
		return Phone_EndCall(playerid, "falha_na_chamada", 5);

	new targetid;

	if(number == 911)
	{
		CellphoneData[playerid][cScreen] = PHONE_PLAYER_CALL;
		PlayerData[playerid][pEmergency] = 1;

		SendClientMessageEx(playerid, COLOR_YELLOW, "(celular) %s: San Andreas Emergency Center.", Phone_GetContact(playerid, number));
		SendClientMessageEx(playerid, COLOR_YELLOW, "(celular) %s: Por favor, relate o ocorrido o mais breve e completo possível.", Phone_GetContact(playerid, number));
	}
	else if(number == 144)
	{
		CellphoneData[playerid][cScreen] = PHONE_PLAYER_CALL;

		if(Get_TaxiOnDuty() == -1)
		{
			SendClientMessageEx(playerid, COLOR_YELLOW, "(celular) %s: Nenhum taxista em trabalho agora, ligue-nos mais tarde.", Phone_GetContact(playerid, number));
			return Phone_EndCall(playerid, "chamada_finalizada");
		}

		PlayerData[playerid][pTaxiCalled] = 1;
		SendClientMessageEx(playerid, COLOR_YELLOW, "(celular) %s: Nossos motorista foram notificados, permaneça neste local.", Phone_GetContact(playerid, number));
	
		SendJobMessage(EMPREGO_Taxista, COLOR_YELLOW, "Central Taxi: %s está pedindo um táxi em %s (use /aceitartaxi para aceitar).", ReturnName(playerid, 0), GetPlayerLocation(playerid));
	
		SetPVarInt(playerid, "MarcarDestino_Taxi", 1);
		SendFormatMessage(playerid, COLOR_RP2, "DICA", "Você pode informar o destino da sua viagem na opção 'Mapa' no menu do jogo.");
	
		Phone_EndCall(playerid, "chamada_finalizada");
	}
	else if(number == 115)
	{
		CellphoneData[playerid][cScreen] = PHONE_PLAYER_CALL;

		PlayerData[playerid][pMechanicCalled] = 1;

		SendClientMessageEx(playerid, COLOR_YELLOW, "(celular) %s: Obrigado por ligar, nossos mecânicos já foram notificados.", Phone_GetContact(playerid, number));
		SendJobMessage(EMPREGO_Mecanico, COLOR_YELLOW, "Central Mecânica: Atenção: %s está pedindo um mecânico em %s (use /aceitarmecanico para aceitar).", ReturnName(playerid, 0), GetPlayerLocation(playerid));
	
		Phone_EndCall(playerid, "chamada_finalizada");
	}
	else if(number == BLACKMARKET_NUMBER)
	{	
		CellphoneData[playerid][cScreen] = PHONE_PLAYER_CALL;

		PlayerData[playerid][pBlackMarket] = 1;

		SendClientMessageEx(playerid, COLOR_YELLOW, "(celular) %s: E ae mano, qual é a boa de hoje? (\"Drogas\" ou \"Armas\")", Phone_GetContact(playerid, number));
	}
	else if(number == 141)
	{
		CellphoneData[playerid][cScreen] = PHONE_PLAYER_CALL;

		PlayerData[playerid][pInsurance] = 1;

		SendClientMessageEx(playerid, COLOR_YELLOW, "(celular) %s: Central de Seguros San Andreas. Por favor, informe a placa do veículo. (Formato: ABC-1234)", Phone_GetContact(playerid, number));
	}
	else if((targetid = CellphoneData[playerid][cCaller]) != INVALID_PLAYER_ID)
	{
		if(CellphoneData[targetid][cScreen] == PHONE_PLAYER_CALLING || CellphoneData[targetid][cScreen] == PHONE_PLAYER_CALL || !CellphoneData[targetid][cSignal])
			return Phone_EndCall(playerid, "linha_ocupada", 4);

		CellphoneData[targetid][cScreen] = PHONE_PLAYER_CALLING;
		CellphoneData[targetid][cCaller] = playerid;
		CellphoneData[targetid][cNumber] = PlayerData[playerid][pCellphone];
		CellphoneData[targetid][cDuration] = 0;

		if(!CellphoneData[targetid][cOpened])
		{
			CellphoneData[targetid][cOpened] = true;
			TextDrawShowForPlayer(targetid, Global_PhoneSystem[PHONE_MAIN_BLACK]);
			TextDrawShowForPlayer(targetid, Global_PhoneSystem[PHONE_MAIN_BLACK_HOME]);
			PlayerTextDrawShow(targetid, Player_PhoneSystem[targetid][PHONE_HOUR]);
		}

		Phone_UpdateScreen(targetid, "call");

		for(new i = PHONE_BLOCKED_ICON; i < MAX_GLOBAL_PHONE_TEXTDRAW; i++)
			TextDrawHideForPlayer(targetid, Global_PhoneSystem[i]);

		for(new i = PHONE_CALLER_RECENTS; i < MAX_PLAYER_PHONE_TEXTDRAW; i++)
			PlayerTextDrawHide(targetid, Player_PhoneSystem[targetid][i]);

		PlayerTextDrawSetString(targetid, Player_PhoneSystem[targetid][PHONE_CALLER_CALL_NAME], FixASCII(Phone_GetContact(targetid, number)));

		PlayerTextDrawShow(targetid, Player_PhoneSystem[targetid][PHONE_CALLER_CALL_NAME]);
		TextDrawShowForPlayer(targetid, Global_PhoneSystem[PHONE_CALLER_CALL_ACCEPT]);
		TextDrawShowForPlayer(targetid, Global_PhoneSystem[PHONE_CALLER_CALL_DECLINE]);

		SendFormatMessage(targetid, COLOR_YELLOW, "Dica", "Para atender ou rejeitar a chamada, utilize o comando '/cursor' para habilitar o cursor.");

		HandlePhoneRing(targetid);

		CellphoneData[targetid][cTimerCalling] = SetTimerEx("CallerApp_Call_Ignored", 30000, false, "dd", targetid, playerid);
	}
	else
	{
		Phone_EndCall(playerid, "número_inativo", 8);
	}

	return 1;
}

