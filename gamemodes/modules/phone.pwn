this::HandlePhoneRing(playerid)
{
	if(CellphoneData[playerid][cNumber] != -1 && CellphoneData[playerid][cScreen] == PHONE_PLAYER_CALLING)
	{
		if(CellphoneData[playerid][cCallNotification] == 0)
		{
			switch(CellphoneData[playerid][cRingtone])
			{
				case 0: PlayerPlaySoundEx(playerid, 1142);
				case 1: PlayerPlaySoundEx(playerid, 1062);
				case 2: PlayerPlaySoundEx(playerid, 1068);
				case 3: PlayerPlaySoundEx(playerid, 1076);
				case 4: PlayerPlaySoundEx(playerid, 1097);
				case 5: PlayerPlaySoundEx(playerid, 1183);
				case 6: PlayerPlaySoundEx(playerid, 1185);
				case 7: PlayerPlaySoundEx(playerid, 1187);
			}

			SendNearbyMessage(playerid, 4.0, COLOR_PURPLE, "> O celular de %s está vibrando", ReturnName(playerid, 0));
		}
		else if(CellphoneData[playerid][cCallNotification] == 1)
		{
			SendNearbyMessage(playerid, 4.0, COLOR_PURPLE, "> O celular de %s está vibrando.", ReturnName(playerid, 0));
		}

		SetTimerEx("HandlePhoneRing", 3000, false, "i", playerid);
		return 1;
	}
	PlayerPlaySoundEx(playerid, 0);

	return 0;
}

