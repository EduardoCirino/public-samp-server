			}
			case 4:
			{
				PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL_TEXT]);

				PlayerTextDrawSetString(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL], "mdl-2000:white-signal-4");
				PlayerTextDrawShow(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL]);
			}
		}
	}
	else
	{
		switch(CellphoneData[playerid][cSignal])
		{
			case 0:
			{
				if(CellphoneData[playerid][cAirPlaneMode])
				{	PlayerTextDrawHide(playerid, Player_PhoneSystem[playerid][PHONE_SIGNAL_TEXT]);

